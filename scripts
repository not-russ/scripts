    "Green Aura",
    "Inferno",
    "Lightning",
    "Power Lightning",
    "Purple Aura",
    "Purple Nova",
    "Red Aura",
    "Supernova",
    "Ultra Inferno",
    "Ultra Mirage",
    "Unstable Mirage",
    "Yellow Aura"
}

for _, aura in ipairs(auraList) do
    auraDropdown:Add(aura)
end

